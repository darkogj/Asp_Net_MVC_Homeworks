
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Fruits</title>
</head>
<body>
    <div>
        <ol>
            @foreach (var fruit in ViewBag.Fruits)
            {
                <li>@fruit</li>
            }
        </ol>
    </div>
</body>
</html>
